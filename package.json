{
	"name": "some-sass",
	"displayName": "Some Sass",
	"description": "Provides code suggestions, documentation and code navigation for modern SCSS",
	"version": "2.7.1",
	"publisher": "SomewhatStationery",
	"license": "MIT",
	"engines": {
		"vscode": "^1.66.0"
	},
	"icon": "icon.png",
	"homepage": "https://github.com/wkillerud/vscode-scss/blob/main/README.md",
	"repository": {
		"type": "git",
		"url": "https://github.com/wkillerud/vscode-scss"
	},
	"keywords": [
		"scss",
		"sassdoc",
		"autocompletion",
		"intellisense",
		"refactor"
	],
	"categories": [
		"Programming Languages"
	],
	"activationEvents": [
		"onLanguage:scss",
		"onLanguage:vue",
		"onLanguage:svelte",
		"onLanguage:astro",
		"onCommand:_somesass.applyExtractCodeAction"
	],
	"capabilities": {
		"virtualWorkspaces": true
	},
	"browser": "./dist/browser-client.js",
	"main": "./dist/node-client.js",
	"contributes": {
		"configuration": {
			"properties": {
				"somesass.scannerDepth": {
					"type": "number",
					"default": 30,
					"description": "The maximum number of nested directories to scan."
				},
				"somesass.scannerExclude": {
					"type": "array",
					"items": {
						"type": "string"
					},
					"default": [
						"**/.git/**",
						"**/node_modules/**",
						"**/bower_components/**"
					],
					"description": "List of glob patterns for directories that are excluded when scanning."
				},
				"somesass.scanImportedFiles": {
					"type": "boolean",
					"default": true,
					"description": "Allows scan imported files."
				},
				"somesass.showErrors": {
					"type": "boolean",
					"default": false,
					"description": "Allows to display errors."
				},
				"somesass.suggestAllFromOpenDocument": {
					"type": "boolean",
					"default": false,
					"description": "VS Code has built-in code suggestions for symbols declared in the open document. If you prefer the suggestions from Some Sass, you can opt in by turning on this setting. There will be duplicates."
				},
				"somesass.suggestFromUseOnly": {
					"type": "boolean",
					"default": false,
					"description": "If your project uses the new module system with @use and @forward, you may want to only include suggestions from your used modules."
				},
				"somesass.suggestVariables": {
					"type": "boolean",
					"default": true,
					"description": "Allows prompt Variables."
				},
				"somesass.suggestMixins": {
					"type": "boolean",
					"default": true,
					"description": "Allows prompt Mixins."
				},
				"somesass.suggestFunctions": {
					"type": "boolean",
					"default": true,
					"description": "Allows prompt Functions."
				},
				"somesass.suggestFunctionsInStringContextAfterSymbols": {
					"type": "string",
					"default": " (+-*%",
					"description": "Allows prompt Functions in String context after specified symbols."
				}
			}
		}
	},
	"devDependencies": {
		"@nodelib/fs.macchiato": "^1.0.4",
		"@types/color": "^3.0.3",
		"@types/color-name": "^1.1.1",
		"@types/micromatch": "^4.0.0",
		"@types/mocha": "^9.1.1",
		"@types/node": "^16.11.43",
		"@types/sinon": "^10.0.12",
		"@types/vscode": "^1.66.0",
		"@types/webpack-env": "1.17.0",
		"@typescript-eslint/eslint-plugin": "^5.31.0",
		"@typescript-eslint/parser": "^5.31.0",
		"@vscode/test-electron": "^2.1.5",
		"@vscode/test-web": "^0.0.32",
		"assert": "2.0.0",
		"eslint": "^8.19.0",
		"eslint-config-prettier": "8.5.0",
		"eslint-plugin-import": "2.26.0",
		"eslint-plugin-prettier": "4.2.1",
		"husky": "^8.0.2",
		"lint-staged": "13.0.3",
		"mocha": "^10.0.0",
		"nyc": "^15.1.0",
		"patch-package": "^6.5.0",
		"prettier": "^2.8.1",
		"rimraf": "^3.0.2",
		"sinon": "15.0.0",
		"ts-loader": "^9.3.1",
		"typescript": "^4.7.4",
		"webpack": "^5.73.0",
		"webpack-cli": "4.10.0"
	},
	"dependencies": {
		"color": "^4.2.3",
		"css-color-names": "^1.0.1",
		"fast-glob": "^3.2.11",
		"path-browserify": "1.0.1",
		"process": "0.11.10",
		"scss-sassdoc-parser": "^1.0.2",
		"scss-symbols-parser": "^2.0.1",
		"util": "^0.12.5",
		"vscode-css-languageservice": "^6.0.1",
		"vscode-languageclient": "^8.0.1",
		"vscode-languageserver": "^8.0.1",
		"vscode-languageserver-textdocument": "^1.0.5",
		"vscode-languageserver-types": "^3.17.1",
		"vscode-uri": "^3.0.3"
	},
	"lint-staged": {
		"src/**/*.ts": [
			"prettier --write",
			"eslint --fix"
		]
	},
	"scripts": {
		"prepare": "husky install",
		"lint-staged": "lint-staged",
		"vscode:prepublish": "npm run build",
		"clean": "rimraf dist",
		"build": "npm run clean && webpack --mode production --devtool hidden-source-map",
		"dev": "npm run clean && webpack --mode development --watch",
		"lint": "eslint \"src/**/*.ts\" --cache",
		"test": "npm run test:clean && npm run test:compile && npm run test:unit",
		"test:clean": "rimraf out",
		"test:compile": "tsc -p ./",
		"test:unit": "mocha \"out/**/*.spec.js\"",
		"test:coverage": "nyc npm run test:unit",
		"test:e2e": "npm run build && node ./out/test/e2e/runTest.js",
		"test:chrome": "npm run build && vscode-test-web --browserType=chromium --extensionDevelopmentPath=. --extensionTestsPath=dist/test/web/suite/index.js ./fixtures/e2e",
		"open:chrome": "npm run build && vscode-test-web --browserType=chromium --extensionDevelopmentPath=. ./fixtures/e2e",
		"postinstall": "patch-package"
	},
	"__metadata": {
		"id": "6d35099c-3671-464c-ac0b-34a0c3823927",
		"publisherDisplayName": "Somewhat Stationery",
		"publisherId": "02638283-c13a-4acf-9f26-24bdcfdfce24",
		"isPreReleaseVersion": false
	}
}
